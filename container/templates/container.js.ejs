/**
 * <%= pascalName %> Container
 */

import React, { Component } from 'react';
import PropTypes from 'prop-types';
<%if (appConfig.usesRedux) { -%>
import { connect } from 'react-redux';
<%}-%>
<%if (hasStyleSheet) { -%>
import '<%= snakeName %>.css';
<%}-%>

class <%= pascalName %> extends Component {
  render() {
    return (
      <div>
        This is <%= pascalName %> container!
      </div>
    );
  }
}

// This is like your component method signature. If you pass any props into this update both.
<%= pascalName %>.propTypes = {};
<%= pascalName %>.defaultProps = {};

<%if (appConfig.usesRedux) { -%>
function mapStateToProps(state) {
  return {
    state,
  };
}

function mapDispatchToProps(dispatch) {
  return {
    dispatch,
  };
}

export default connect(mapStateToProps, mapDispatchToProps)(<%= pascalName %>);
<% } else{ -%>
export default <%= pascalName %>
<% } -%>
